<div class="">
  <h2>Below are all the connected androids</h2>
</div>
<% for(var i = 0; i < sockets.length; i++){ %>
  <div class="connectionBox">
    <table>
      <tbody>
        <tr>
          <td>
            <p><%= sockets[i] %></p>
          </td>
        </tr>
        <tr>
          <td>
            <p id="pinglabel-<%= sockets[i] %>">
              <div class="loader"></div>
            </p>
          </td>
        </tr>
        <tr>
          <td><input id="pingbutton-<%= sockets[i] %>" type="button" class="btn" value="Ping!" style="margin-top: 30px;" onclick="pingding('<%=sockets[i]%>')"></td>
        </tr>
      </tbody>
    </table>
  </div>
  <%}%>
